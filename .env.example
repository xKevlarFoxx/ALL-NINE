# Mobile Money API Endpoints
MPESA_API_ENDPOINT=https://api.mpesa.com/v1
TIGO_API_ENDPOINT=https://api.tigo.co.tz/v1
AIRTEL_API_ENDPOINT=https://api.airtel.co.tz/v1

# SMS Provider
SMS_PROVIDER_API_KEY=your_sms_provider_api_key
SMS_PROVIDER_SECRET=your_sms_provider_secret

# Tanzania Revenue Authority
TRA_TIN=123456789
TRA_API_ENDPOINT=https://api.tra.go.tz/v1
TRA_API_KEY=your_tra_api_key

# Tanzania Electronic Payment Scheme
TEPS_MERCHANT_ID=your_merchant_id
TEPS_API_KEY=your_teps_api_key
TEPS_API_ENDPOINT=https://api.teps.co.tz/v1

# File Storage
MAX_FILE_SIZE=5242880 # 5MB in bytes
ALLOWED_FILE_TYPES=image/jpeg,image/png,application/pdf

# Security
JWT_SECRET=your_jwt_secret
ENCRYPTION_KEY=your_encryption_key

# Analytics
ANALYTICS_API_KEY=your_analytics_api_key

# Development
DEBUG=false
API_TIMEOUT=30000 # 30 seconds